{
  "name": "@worker-tools/request-cookie-store",
  "version": "0.4.3",
  "description": "An implementation of the Cookie Store API for request handlers",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": "./index.js",
    "./set-cookie": "./set-cookie.js"
  },
  "files": [
    "src",
    "*.js*",
    "*.d.ts*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -f *.d.ts *.js *.js.map *.d.ts.map",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build": "tsc -d",
    "watch": "tsc -d -w",
    "prepack": "npm run clean && npm run build"
  },
  "author": "Florian Klampfer <mail@qwtel.com> (https://qwtel.com/)",
  "license": "MIT",
  "dependencies": {
    "cookie-store-interface": "^0.1.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@worker-tools/location-polyfill": "workspace:^0.3.2-pre.1",
    "jest": "^27.5.1",
    "jest-environment-miniflare": "^2.3.0",
    "miniflare": "^2.3.0",
    "typescript": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/worker-tools/request-cookie-store.git"
  },
  "bugs": {
    "url": "https://github.com/worker-tools/request-cookie-store/issues"
  },
  "homepage": "https://github.com/worker-tools/request-cookie-store#readme",
  "keywords": [
    "cookies",
    "cookie-store",
    "middleware",
    "cloudflare",
    "cloudflare-workers",
    "standards-adherence"
  ]
}
